[alias]
  s = status
  f = fetch -a -p
  l = pull
  lr = pull --rebase
  h = push
  st = stash
  co = commit
  ca = commit -a

  # List tags
  tags = tag -l

  # List branches
  branches = branch -a

  # List remotes
  remotes = remote -v

[user]
  name = Aaron Miller
  email = "aaron@aaron-miller.net"

[init]
  defaultBranch = main
